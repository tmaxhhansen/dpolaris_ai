{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://dpolaris.ai/schemas/run_summary.schema.json",
  "title": "RunSummary",
  "type": "object",
  "required": [
    "run_id",
    "status",
    "created_at",
    "started_at",
    "completed_at",
    "duration_seconds",
    "git_commit_hash",
    "git_branch",
    "environment",
    "hostname",
    "user",
    "model_type",
    "target",
    "horizon",
    "tickers",
    "timeframes"
  ],
  "properties": {
    "run_id": {
      "type": "string"
    },
    "status": {
      "type": "string"
    },
    "created_at": {
      "type": [
        "string",
        "null"
      ]
    },
    "started_at": {
      "type": [
        "string",
        "null"
      ]
    },
    "completed_at": {
      "type": [
        "string",
        "null"
      ]
    },
    "duration_seconds": {
      "type": [
        "number",
        "null"
      ]
    },
    "git_commit_hash": {
      "type": [
        "string",
        "null"
      ]
    },
    "git_branch": {
      "type": [
        "string",
        "null"
      ]
    },
    "environment": {
      "type": "object",
      "additionalProperties": true
    },
    "hostname": {
      "type": [
        "string",
        "null"
      ]
    },
    "user": {
      "type": [
        "string",
        "null"
      ]
    },
    "model_type": {
      "type": "string"
    },
    "target": {
      "type": "string"
    },
    "horizon": {
      "type": "integer"
    },
    "tickers": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "timeframes": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "universe_hash": {
      "type": [
        "string",
        "null"
      ]
    }
  },
  "additionalProperties": true
}