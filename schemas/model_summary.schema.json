{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://dpolaris.ai/schemas/model_summary.schema.json",
  "title": "ModelSummary",
  "type": "object",
  "required": [
    "algorithm",
    "hyperparameters",
    "feature_importance",
    "calibration_method"
  ],
  "properties": {
    "algorithm": { "type": "string" },
    "hyperparameters": { "type": "object", "additionalProperties": true },
    "feature_importance": {
      "type": "array",
      "items": { "type": ["object", "array"], "additionalProperties": true }
    },
    "calibration_method": { "type": ["string", "null"] }
  },
  "additionalProperties": true
}
